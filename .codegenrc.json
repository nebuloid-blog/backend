{
  "schema": "./src/schema.gql",
	"require": [
		"ts-node/register"
	],
	"generates": {
		"./src/generated/types.d.ts": {
			"config": {
				"mappers": {
					"User": "UserDbObject",
					"Course": "CourseDbObject",
					"Project": "ProjectDbObject"
				}
			},
			"plugins": [
				"typescript",
				"typescript-mongodb",
				"typescript-resolvers"
			]
		}
	}
}
