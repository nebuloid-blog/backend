{
	"root": true,
	"env": {
		"node": true,
		"es2022": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "./tsconfig.json",
		"sourceType": "module",
		"ecmaVersion": "latest",
		"ecmaFeatures": {
			"impliedStrict": true
		}
	},
	"plugins": ["@typescript-eslint"],
	"extends": ["tabbify-typescript"],
	"rules": {
		// Temporary rule for {'*': ...} in a dictionary.
		"@typescript-eslint/naming-convention": "off"
	}
}
