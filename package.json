	"contributors": [ ],
	"packageManager": "yarn@3.2.1",
	"main": "",
	"type": "module",
	"scripts": {
		"start": "yarn codegen && yarn lint-gen --fix; yarn indent-gen && ts-node --esm src/server.ts",
		"dev": "nodemon --exec 'yarn start'",
		"codegen": "graphql-codegen --config .codegenrc.json",
		"lint": "eslint ./src/**/*.ts",
		"lint-gen": "eslint ./src/generated/**/*.ts --no-ignore --rule '@typescript-eslint/consistent-type-definitions: 0'",
		"indent": "replace-in-files --string='  ' --replacement='\t'",
		"indent-gen": "yarn indent ./src/generated/**/*.ts"
	},
	"dependencies": {
		"@graphql-codegen/typescript-mongodb": "^2.4.2",
		"bcrypt": "^5.0.1",
		"dotenv": "^16.0.1",
		"express": "^4.18.1",
		"express-graphql": "^0.12.0",
		"graphql": "^16.5.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^6.5.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "2.11.3",
		"@graphql-codegen/typescript": "2.7.2",
		"@graphql-codegen/typescript-resolvers": "2.7.2",
		"@graphql-tools/schema": "^8.5.1",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/node": "^18.6.3",
		"@typescript-eslint/eslint-plugin": "^5.32.0",
		"@typescript-eslint/parser": "^5.32.0",
		"eslint": "^8.21.0",
		"eslint-config-tabbify-typescript": "^0.1.7",
		"eslint-plugin-import": "^2.26.0",
		"nodemon": "^2.0.19",
		"replace-in-files-cli": "^2.0.0",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	}
}
