	"license": "MIT",
	"version": "0.0.0",
	"description": "GraphQL-based backend for my portfolio.",
	"keywords": [ ],
	"homepage": "https://github.com/noltron000-portfolio/backend#README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/noltron000-portfolio/backend.git"
	},
	"bugs": {
		"url": "https://github.com/noltron000-portfolio/backend/issues",
		"email": "noltron000@gmail.com"
	},
	"author": {
		"name": "noltron000",
		"email": "noltron000@gmail.com",
		"url": "https://noltron000.com/"
	},
	"contributors": [ ],
	"packageManager": "yarn@3.2.1",
	"main": "",
	"type": "module",
	"scripts": {
		"start": "yarn codegen && yarn lint-gen --fix; yarn indent-gen && ts-node --esm src/server.ts",
		"dev": "nodemon --exec 'yarn start'",
		"codegen": "graphql-codegen --config .codegenrc.json",
		"lint": "eslint ./src/**/*.ts",
		"lint-gen": "eslint ./src/generated/**/*.ts --no-ignore --rule '@typescript-eslint/consistent-type-definitions: 0'",
		"indent": "replace-in-files --string='  ' --replacement='\t'",
		"indent-gen": "yarn indent ./src/generated/**/*.ts"
	},
	"dependencies": {
		"express": "^4.18.1",
		"express-graphql": "^0.12.0",
		"graphql": "^16.5.0"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "2.6.2",
		"@graphql-codegen/typescript": "2.5.1",
		"@graphql-codegen/typescript-resolvers": "2.6.6",
		"@types/express": "^4.17.13",
		"@typescript-eslint/eslint-plugin": "^5.30.7",
		"@typescript-eslint/parser": "^5.30.7",
		"eslint": "^8.20.0",
		"eslint-config-tabbify-typescript": "^0.1.6",
		"eslint-plugin-import": "^2.26.0",
		"nodemon": "^2.0.16",
		"replace-in-files-cli": "^2.0.0",
		"ts-node": "^10.8.1",
		"typescript": "^4.7.4"
	}
}
